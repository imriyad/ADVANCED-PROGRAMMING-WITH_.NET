@model List<StudentManagement.DAL.Models.Student>

<h2>Student List</h2>
<a href="/Student/Create">Add New Student</a>
<table border="1">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
        <th>Email</th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.StudentId</td>
            <td>@s.FullName</td>
            <td>@s.Age</td>
            <td>@s.Email</td>
        </tr>
    }
</table>
